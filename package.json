{
  "name": "webpack-tree",
  "displayName": "webpack tree",
  "description": "This extension can show the dependency tree of your webpack project.",
  "version": "0.0.15",
  "publisher": "xux2020",
  "engines": {
    "vscode": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uoau/webpack-tree"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "main.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.getEntryWebpack",
        "title": "webpack tree:load json"
      },
      {
        "command": "extension.editNote",
        "title": "Edit",
        "icon": {
          "light": "img/edits1.svg",
          "dark": "img/edits2.svg"
        }
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.getEntryWebpack",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "extension.editNote",
          "group": "inline"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "treeView",
          "title": "WEBPACK TREE",
          "icon": "img/tree2.svg"
        }
      ]
    },
    "views": {
      "treeView": [
        {
          "id": "treeView-item",
          "name": ""
        }
      ]
    }
  }
}
